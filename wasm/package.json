{
  "author": "You <you@example.com>",
  "name": "rust-webpack-template",
  "version": "0.1.0",
  "scripts": {
    "deploy": "wasm-pack build --release --no-typescript -t web --out-name \"tlang-wasm\"",
    "build": "rimraf dist && wasm-pack build -t web --out-name \"tlang-wasm\" && webpack",
    "start": "rimraf dist && wasm-pack build -t web --out-name \"tlang-wasm\" && webpack-dev-server",
    "test": "cargo test && wasm-pack test --headless"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^5.0.3",
    "rimraf": "^3.0.0",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  }
}
